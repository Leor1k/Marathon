                </Button>
                <Rectangle Margin="602,139,38,220" Stroke="Black"/>
                <Image x:Name="image_mi_im" HorizontalAlignment="Left" Height="188" Margin="606,149,0,0" VerticalAlignment="Top" Width="292" StretchDirection="DownOnly"/>
